<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

        <bean id="course" class="com.cjc.model.Course">
         <property name="cname" value="Advance Java"/>
         <property name="price" value="22000"/>
        </bean>
         
         <bean id="stu" class="com.cjc.model.Student">
           <property name="rollno" value="101"/>
           <property name="name" value="Saurabh"/>
           <property name="address" value="Nagar"/>
           <property name="course"  ref="course"/>
         </bean>
</beans>
